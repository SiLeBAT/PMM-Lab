<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
	<extension point="org.eclipse.core.runtime.preferences">
    	<initializer class="de.dim.bfr.knime.preferences.CustomRPreferenceInitializer">
      	</initializer>
    </extension>

    <extension point="org.eclipse.ui.preferencePages">
        <page category="org.knime.workbench.ui.preferences" class="de.dim.bfr.knime.preferences.CustomRPreferencePage" id="org.knime.ext.r.preferences.RPreferencePage" name="BfR Plugin">
        </page>
    </extension>
   
    <extension point="org.knime.workbench.repository.categories">
      <category description="BfR" level-id="bfr" name="BfR Nodes" path="/"/>
      <category
            icon="icons/missing.gif"
            level-id="statup"
            name="Statup"
            path="/bfr">
      </category>
  </extension>
   
   <extension point="org.knime.workbench.repository.nodes">
   <!--
      <node category-path="/bfr" factory-class="de.man.ManipulatorNodeFactory" id="Manipulator">
      </node>
      -->
      <node category-path="/bfr/statup" factory-class="de.dim.bfr.knime.nodes.r.view.CustomRViewNodeFactory" id="CustomRView"/>
	  <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.tablefilter.TableFilterNodeFactory" id="Table Filter"/>
	  <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.clone.ModelClonerNodeFactory" id="Tablemodel Cloner"/>
	  <!--
      <node category-path="/bfr" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.interactivetable.BfRInteractiveTableNodeFactory" id="InteractiveTable"/>
      -->
      <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.db.SimpleDatabaseReaderNodeFactory" id="SimpleDBReader"/>
      <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.db.DatabaseArrayWriterNodeFactory" id="DatabaseArrayWriter"/>
	  <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.split.TableArraySplitterNodeFactory" id="TableArraySplitter"/>
      <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.merge.tabletoarray.TableArrayMergerNodeFactory" id="TableArrayMerger"/>
	  <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.merge.arraytoarray.ArrayToArrayMergerNodeFactory" id="ArrayToArrayMerger"/>
	  <node category-path="/bfr/statup" factory-class="de.dim.bfr.knime.nodes.loadrelateddata.LoadRelatedDataNodeFactory" id="LoadRelatedData"/>
	  <node category-path="/bfr/statup" factory-class="de.dim.bfr.knime.nodes.filterdispatcher.FilterDispatcherNodeFactory" id="Filter Dispatcher"/>
      <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.r.sec.estimate.SecondaryEstimationNodeFactory" id="Secondary Estimation"/>
	  <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.r.sec.forecast.SecondaryForecasterNodeFactory" id="Secondary Forecasting"/>
	  <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.r.prim.forecast.PrimaryForecasterNodeFactory" id="Primary Forecasting"/>
	  <node category-path="/bfr/statup" expert-flag="false" factory-class="de.dim.bfr.knime.nodes.r.prim.estimate.PrimaryEstimationNodeFactory" id="Primary Estimation"/>
	  
</extension>
   <!--extension point="org.knime.workbench.repository.metanode">
      <PersistedMetaNode category-path="/" expert-flag="false" id="BfRPlugin.PersistedMetaNode2" name="MetaTest" workflowDir="MetaTestNode/">
      </PersistedMetaNode>
   </extension-->
       
</plugin>
