<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="de.bund.bfr.knime.pmm.ui.commands.category"
            name="PMM-Lab">
      </category>
      <command
            categoryId="de.bund.bfr.knime.pmm.ui.commands.category"
            id="de.bund.bfr.knime.pmm.ui.commands.openDBGUI"
            name="Open DB Gui...">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="de.bund.bfr.knime.pmm.ui.handlers.OpenDBGUI"
            commandId="de.bund.bfr.knime.pmm.ui.commands.openDBGUI">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="de.bund.bfr.knime.pmm.ui.menus.masterdata"
               label="PMM-Lab"
               mnemonic="M">
            <command
                  commandId="de.bund.bfr.knime.pmm.ui.commands.openDBGUI"
                  id="de.bund.bfr.knime.pmm.ui.menus.openDBGUI"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/BfRPMM.png"
            level-id="pmmlab"
            name="PMM-Lab"
            path="/">
      </category>
      <category
            icon="icons/Editors.png"
            level-id="editors"
            name="Editors"
            path="/pmmlab">
      </category>
      <category
            icon="icons/Model.png"
            level-id="fitting"
            name="Model Fitting &amp; Prediction"
            path="/pmmlab">
      </category>
      <category
            icon="icons/Reader.png"
            level-id="reader"
            name="Reader"
            path="/pmmlab">
      </category>
      <category
            icon="icons/SelectionAndViews.png"
            level-id="views"
            name="Selection &amp; Views"
            path="/pmmlab">
      </category>
      <category
            icon="icons/Writer.png"
            level-id="writer"
            name="Writer"
            path="/pmmlab">
      </category>
   </extension>

</plugin>
