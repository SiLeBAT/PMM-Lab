<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>

<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/pmmlab/editors" factory-class="de.bund.bfr.knime.pmm.manualmodelconf.ManualModelConfNodeFactory" id="de.bund.bfr.knime.pmm.manualmodelconf.ManualModelConfNodeFactory"/>
   <node category-path="/pmmlab/editors" factory-class="de.bund.bfr.knime.pmm.manualmodelconf.ManualModelEditorNodeFactory" id="de.bund.bfr.knime.pmm.manualmodelconf.ManualModelEditorNodeFactory"/>
   <node
         category-path="/pmmlab/editors"
         factory-class="de.bund.bfr.knime.pmm.manualmodelconf.ManualFormulaCreatorNodeFactory"
         id="de.bund.bfr.knime.pmm.manualmodelconf.ManualFormulaCreatorNodeFactory">
   </node>
</extension>
   
</plugin>
