<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="./ComBaseWriter.png" type="Sink">
    <name>ComBase Writer</name>
    
    <shortDescription>
        Writes ComBase CSV files
    </shortDescription>
    
    <fullDescription>
        <intro>Save microbial data from the workflow as CSV file. The structure is identical to the <a href="http://www.combase.cc">ComBase</a> data structure.</intro>
        
        
        <option name="Write ComBase CSV file">Browse your folders and define the file name to save microbial data as ComBase CSV file.</option>
    </fullDescription>
    
    <ports>
        <inPort index="0" name="Microbial data table">Microbial data table</inPort>
    </ports>        
</knimeNode>
