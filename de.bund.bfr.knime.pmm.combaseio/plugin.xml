<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/pmmlab/data" factory-class="de.bund.bfr.knime.pmm.combaseio.CombaseReaderNodeFactory" id="de.bund.bfr.knime.combaseio.CombaseReaderNodeFactory"/>
   <node category-path="/pmmlab/data" factory-class="de.bund.bfr.knime.pmm.combaseio.CombaseWriterNodeFactory" id="de.bund.bfr.knime.combaseio.CombaseWriterNodeFactory"/>
</extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/BfRPMM.png"
            level-id="pmmlab"
            name="PMM-lab"
            path="/">
      </category>
      <category
            icon="icons/Data.png"
            level-id="data"
            name="Data"
            path="/pmmlab">
      </category>
      <category
            icon="icons/Model.png"
            level-id="model"
            name="Model"
            path="/pmmlab">
      </category>
      <category
            icon="icons/Forecast.png"
            level-id="forecast"
            name="Forecast"
            path="/pmmlab">
      </category>
   </extension>
   
</plugin>
