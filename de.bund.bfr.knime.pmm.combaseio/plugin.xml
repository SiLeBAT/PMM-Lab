<?eclipse version="3.0"?>

<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/pmmlab/reader" factory-class="de.bund.bfr.knime.pmm.combaseio.CombaseReaderNodeFactory" id="de.bund.bfr.knime.combaseio.CombaseReaderNodeFactory"/>
   <node category-path="/pmmlab/writer" factory-class="de.bund.bfr.knime.pmm.combaseio.CombaseWriterNodeFactory" id="de.bund.bfr.knime.combaseio.CombaseWriterNodeFactory"/>
</extension>
   
</plugin>
